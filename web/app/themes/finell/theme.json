{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
    "color": {
      "palette": [
				{
					"color": "#ebebe5",
					"name": "Light",
					"slug": "light"
				},
        {
					"color": "#1b1b1b",
					"name": "Dark",
					"slug": "dark"
				}
      ]
    },
    "typography": {
			"fluid": true,
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.9rem",
					"slug": "sm"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1.05rem",
					"slug": "md"
				},
				{
					"fluid": false,
					"name": "Large",
					"size": "1.85rem",
					"slug": "lg"
				},
				{
					"fluid": false,
					"name": "Extra Large",
					"size": "2.5rem",
					"slug": "xl"
				},
				{
					"fluid": false,
					"name": "Extra Extra Large",
					"size": "3.27rem",
					"slug": "2xl"
				}
			]
		}
  },
  "styles": {
    "typography": {
			"fontFamily": "sans-serif",
			"fontSize": "var(--wp--preset--font-size--md)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.5"
		}
  }
}
